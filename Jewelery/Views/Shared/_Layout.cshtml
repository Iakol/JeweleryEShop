</head>
<body>
    <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <script src="~/js/Cookies/ScreenSize.js"></script>
    <script src="~/lib/jquery.ui.combined/1.13.3/Content/Scripts/jquery-ui-1-13.3.js"></script>
    <script src="~/lib/select2/dist/js/select2.min.js"></script>





    <partial name="~/Views/Header/MainHeader.cshtml" />
    <div class="container-fluid m-0 p-0">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>
    <partial name="~/Views/Footer/Footer.cshtml" />

    
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
